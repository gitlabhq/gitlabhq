= render "head"

- if @path.present?
  %ul.breadcrumb
    %li
      %span.arrow
      = link_to project_commits_path(@project) do
        = @project.name
      %span.divider
        \/
    %li
      %a{href: "#"}= @path.split("/").join(" / ")

%div{id: dom_id(@project)}
  #commits_list= render "commits"
.clear
.loading{ style: "display:none;"}

- if @commits.count == @limit
  :javascript
    $(function(){
      CommitsList.init("#{@ref}", #{@limit});
    });

