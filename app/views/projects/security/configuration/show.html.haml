- breadcrumb_title _("Security configuration")
- page_title _("Security configuration")

#js-security-configuration{ data: { **@configuration.to_html_data_attribute,
  vulnerability_training_docs_path: vulnerability_training_docs_path,
  upgrade_path: security_upgrade_path,
  project_full_path: @project.full_path,
  project_id: @project.id,
  group_full_path: @project.root_ancestor.full_path,
  can_apply_profiles: can?(current_user, :apply_security_scan_profiles, @project).to_s,
  can_read_attributes: can?(current_user, :read_security_attribute, @project.root_ancestor).to_s,
  can_manage_attributes: can?(current_user, :admin_security_attributes, @project.root_ancestor).to_s,
  group_manage_attributes_path: group_configuration_path,
  security_scan_profiles_licensed: @project.licensed_feature_available?(:security_scan_profiles).to_s,
  use_graphql: 'false' } }
