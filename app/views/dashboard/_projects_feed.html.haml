- projects.first(5).each do |project|
  %div.dash_project_item
    = link_to project do 
      %h4
        %span.ico.project
        = truncate project.name, :length => 30
        %small
          last activity at 
          = project.last_activity_date.stamp("Aug 25, 2011")
        %span.right.arrow
          &rarr;
