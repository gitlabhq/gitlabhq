{
  "name": "dj",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run --reporter mocha-allure-reporter"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^1.3.0",
    "@cypress/skip-test": "^2.5.0",
    "cypress": "^4.0.1",
    "cypress-cucumber-preprocessor": "^1.19.2",
    "cypress-failed-log": "^2.5.1",
    "cypress-file-upload": "^3.5.3",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "dependencies": {
    "allure-commandline": "^2.9.0",
    "cypress-wait-until": "^1.6.1",
    "mocha": "^6.2.2",
    "mocha-allure-reporter": "^1.4.0"
  }
}
